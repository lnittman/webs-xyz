{
  "name": "@repo/api",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts",
      "types": "./index.ts"
    },
    "./utils/*": {
      "import": "./utils/*.ts",
      "require": "./utils/*.ts",
      "types": "./utils/*.ts"
    },
    "./constants": {
      "import": "./constants/index.ts",
      "require": "./constants/index.ts",
      "types": "./constants/index.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@mastra/client-js": "^0.10.2",
    "@repo/auth": "workspace:*",
    "@repo/database": "workspace:*",
    "next": "15.3.2",
    "server-only": "^0.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.14.1",
    "typescript": "^5.8.3"
  }
}
